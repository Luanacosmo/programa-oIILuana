v1 = {7, 2, 5, 4, 9}
  [2, 5, 4, 7, 9]
  [2, 4, 5, 7, 9]
v2 = {7, 5, 5, 3, 9}
  [5, 5, 3, 7, 9]
  [5, 3, 5, 7, 9]
  [3, 5, 5, 7, 9]
v3 = {1, 2, 3, 4}
  [1, 2, 3, 4]
v4 = {4, 3, 2, 1}
  [3, 2, 1, 4]
  [2, 1, 3, 4]
  [1, 2, 3, 4]
II- 3 comparações
IV- 
void bubble_r (int *v, int n){
 if (n > 1){
 for (int i = 0; i < n-1; i++)
 if (v[i] > v[i+1])
 troca (&v[i], &v[i+1]);
 bubble_r(v, n-1);
 }
}
